    "visual-tests": "percy exec -- node ./visualTests/visualTests.js",
    "docs": "rm -Rf ./docs/*; ./node_modules/.bin/esdoc",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/xeokit-convert.git"
  },
