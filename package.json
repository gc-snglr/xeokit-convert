    "@zip.js/zip.js": "^2.4.10",
    "earcut": "^2.2.3",
    "fs": "0.0.1-security",
    "jszip": "^3.9.1",
    "jszip-sync": "^3.2.1-sync",
    "npm-upgrade": "^3.1.0",
    "path": "^0.12.7",
    "web-ifc": "^0.0.34",
    "xmldom": "^0.6.0"
